<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="safetyLab.PublishersPage">
    <ContentPage.Content>
        <StackLayout>
            <Label 
                Text="Safety Lab"
                VerticalOptions="StartAndExpand"
                HorizontalOptions="Center"
                TextColor="Black"
                FontSize="Large"/>

            <Label
                Text="Publishers"
                FontAttributes="Bold"
                TextColor="Black"
                VerticalOptions="Center"
                HorizontalOptions="Center"/>
            <Label
                Text="Creator:"
                TextColor="Black"
                VerticalOptions="Center"
                HorizontalOptions="Center"/>
            <Label
                Text="Co-Creator:"
                TextColor="Black"
                VerticalOptions="Center"
                HorizontalOptions="Center"/>
            <Label
                Text="Intern:"
                TextColor="Black"
                VerticalOptions="Center"
                HorizontalOptions="Center"/>
                
            <Label
                Text="TRACIE Creator"
                TextColor="Black"
                VerticalOptions="Center"
                HorizontalOptions="Center"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>